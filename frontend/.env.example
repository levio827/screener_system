# API Configuration
VITE_API_BASE_URL=http://localhost:8000/v1

# WebSocket Configuration (FE-005: Real-time Order Book)
# Endpoint: /v1/ws (see BE-006)
# Requires JWT authentication
VITE_WS_URL=ws://localhost:8000/v1/ws

# Environment
VITE_ENV=development

# Analytics Configuration (IMPROVEMENT-011)
# Mixpanel project token (leave empty or 'mock' for development)
VITE_MIXPANEL_TOKEN=
# Enable analytics debug mode (logs events to console)
VITE_ANALYTICS_DEBUG=true
# Disable analytics entirely
VITE_ANALYTICS_DISABLED=false
# Respect browser Do Not Track setting
VITE_ANALYTICS_RESPECT_DNT=true
